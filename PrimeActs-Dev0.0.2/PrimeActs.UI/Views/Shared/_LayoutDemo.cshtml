<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge;" />
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <meta charset="utf-8">
    <meta name="viewport" content ="width=device-width, initial-scale=1">
    <title>PrimeActs</title>
    <link rel="shortcut icon" type="image/png" href="~/favicon.ico" />
    <script src="~/Scripts/jquery-2.1.4.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
    <script src="~/Scripts/sidebarscript.js"></script>
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/knockout")
    @Styles.Render("~/Content/css/NewCSS")


</head>
<body style="font-family:'expressway rg';">
   

    <footer class="container-fluid">
        <img src="~/Content/images/PrimeActsLogoFinal.png" />
        <ul class="pull-right" style="list-style-type: none;
    margin: 0;
    padding: 0;">
            <li style="display:inline; float: left;">
                <input type="search" />
            </li>
            <li style="display:inline; float: left;">
                <img src="~/Content/images/IconSearchOrange.png" />
            </li>
            <li style="display:inline; float: left;">
                &nbsp;&nbsp;&nbsp;&nbsp;
            </li>
            <li style="display:inline; float: left;">
                @Html.Partial("_LoginPartial")
            </li>
        </ul>
    </footer>
    <nav class="navbar navbar-inverse sidebar" role="navigation">
        <div class="container-fluid">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-sidebar-navbar-collapse-1">
                    <span class="sr-only text-muted">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-pin-classic"></span>
                </button>
                <a class="navbar-brand" href="#">PrimeActs</a>
            </div>
            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-sidebar-navbar-collapse-1">
                <ul class="nav navbar-nav">

                    <li><a href="#">Home<span style="font-size:16px;" class="pull-right hidden-xs showopacity glyphicon glyphicon-home"></span></a></li>
                    <li><a href="#">Customers<span style="font-size:16px;" class="pull-right hidden-xs showopacity glyphicon glyphicon-user"></span></a></li>
                    <li>
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">Suppliers<span class="caret"></span><span style="font-size:16px;" class="pull-right showopacity glyphicon glyphicon-barcode"></span></a>
                        <ul class="dropdown-menu">
                            <li>&nbsp;&nbsp;&nbsp;<a href="~/Demo/StockBoard">Orders<span style="font-size:16px;" class="pull-right showopacity glyphicon glyphicon-apple"></span></a></li>
                            <li>&nbsp;&nbsp;&nbsp;<a href="~/Consignment/CreateConsignment">Consignments<span style="font-size:16px;" class="pull-right showopacity glyphicon glyphicon-tower"></span></a></li>
                            <li>&nbsp;&nbsp;&nbsp;<a href="#">Invoices<span style="font-size:16px;" class="pull-right showopacity glyphicon glyphicon-cloud"></span></a></li>
                        </ul>
                    </li>
                    <li><a href="#">Nominal<span style="font-size:16px;" class="pull-right hidden-xs showopacity glyphicon glyphicon-bookmark"></span></a></li>
                    <li>
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">Trading<span class="caret"></span><span style="font-size:16px;" class="pull-right hidden-xs showopacity glyphicon glyphicon-briefcase"></span></a>
                        <ul class="dropdown-menu">
                            <li>&nbsp;&nbsp;&nbsp;<a href="~/Demo/StockBoard">Orders<span style="font-size:16px;" class="pull-right showopacity glyphicon glyphicon-apple"></span></a></li>
                            <li>&nbsp;&nbsp;&nbsp;<a href="~/Consignment/CreateConsignment">Consignments<span style="font-size:16px;" class="pull-right showopacity glyphicon glyphicon-tower"></span></a></li>
                            <li>&nbsp;&nbsp;&nbsp;<a href="~/Ticket/CreateTicket">Tickets<span style="font-size:16px;" class="pull-right showopacity glyphicon glyphicon-envelope"></span></a></li>
                            <li>&nbsp;&nbsp;&nbsp;<a href="~/Ticket/CreateTransfer">Transfers<span style="font-size:16px;" class="pull-right showopacity glyphicon glyphicon-envelope"></span></a></li>
                            <li>&nbsp;&nbsp;&nbsp;<a href="~/Invoice/RunInvoice">Invoices<span style="font-size:16px;" class="pull-right showopacity glyphicon glyphicon-cloud"></span></a></li>
                        </ul>



<li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">Settings <span style="font-size:16px;" class="pull-right hidden-xs showopacity glyphicon glyphicon-cog"></span></a>
                        <ul class="dropdown-menu forAnimate" role="menu">
                            <li><a href="#">Action</a></li>
                            <li><a href="#">Another action</a></li>
                            <li><a href="#">Something else here</a></li>
                            <li class="divider"></li>
                            <li><a href="#">Separated link</a></li>
                            <li class="divider"></li>
                            <li><a href="#">One more separated link</a></li>
                        </ul>
                    </li>
                </ul>
            </div>
        </div>


    </nav>
    <div class="main">
        @RenderBody()
</div>
</body>
</html>
  
