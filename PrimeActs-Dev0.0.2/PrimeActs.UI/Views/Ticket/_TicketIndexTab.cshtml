<script src="~/KJScripts/Helper.js"></script>
<script src="~/TypeScripts/Ticket/TicketIndexTab.js"></script>
<script src="~/TypeScripts/Ticket/TicketPartial.js"></script>
<link href="~/Content/css/bootstrap-glyphicons.css" rel="stylesheet" />
<script type="text/javascript">
    //debugger; /////////////////////////////////////
    var CustomerModel = new TicketIndexTabViewModel();
    console.log('TicketIndexTabViewModel  ', CustomerModel);
</script>

<div id="@ViewBag.TicketsPanel" class="container-fluid">
    <div class="col-md-12">
        <div class="panel panel-default">
            <div class="panel-heading clearfix">
                <span class="panel-title">View Recent Tickets</span>
            </div>
            <div class="panel-body">
                @*<div class="clearfix"></div>*@
                @Html.Action("IndexTab", "Ticket", new { forIndex = 1 })
            </div>
        </div>
    </div>
</div>
<script type="text/javascript">
    $(document).ready(function () {
        var identifier = '@ViewBag.TicketsPanel';

        ko.validation.init({
            registerExtenders: true,
            messagesOnModified: true,
            insertMessages: true,
            parseInputAttributes: true,
            messageTemplate: null
        }, true);

        ko.applyBindings(CustomerModel, document.getElementById(identifier));
    });
</script>
